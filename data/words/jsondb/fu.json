[
  {
    "汉": "終に・遂に・竟に",
    "日": "ついに",
    "标": [
      "副"
    ],
    "释": [
      {
        "义": "终于，竟然",
        "例": [
          {
            "日": "敵軍は終に我が軍門に降った",
            "汉": "敌军终于向我军投降了"
          },
          {
            "日": "終に難関を突破した",
            "汉": "终于突破了难关"
          }
        ]
      },
      {
        "义": "最终",
        "注": [
          "后接否定式"
        ],
        "例": [
          {
            "日": "終に彼は来なかった",
            "汉": "他最后也没来"
          },
          {
            "日": "会議が終わるまで終に口をきかなかった",
            "汉": "直到会议结束始终没开口"
          }
        ]
      }
    ]
  },
  {
    "日": "くらい・ぐらい",
    "标": [
      "副"
    ],
    "注": [
      "用『どのぐらい』提问"
    ],
    "释": [
      {
        "义": "照某评估",
        "例": [
          {
            "日": "そんなことくらいできるさ",
            "汉": "那种小事当然能做啦"
          },
          {
            "日": "この問題は小学生でもできるくらい易しい",
            "汉": "这个问题容易得连小学生都会"
          },
          {
            "日": "泰山くらい高い山はない",
            "汉": "没有像泰山那样高的山"
          },
          {
            "日": "異国で病気をするくらい心細いことはない",
            "汉": "没有比在异国他乡生病更令人觉得无依无靠了"
          },
          {
            "日": "彼らの技量は同じくらいだ",
            "汉": "他们的本领不相上下"
          },
          {
            "日": "ここから中山公園まで何のくらいありますか",
            "汉": "从这儿到中山公园大概还有多远"
          }
        ]
      }
    ]
  }
]